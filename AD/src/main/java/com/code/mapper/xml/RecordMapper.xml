<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.code.mapper.RecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.code.pojo.Record">
        <id column="Record_id" property="recordId" />
        <result column="User_id" property="userId" />
        <result column="AD_id" property="adId" />
        <result column="Record_date" property="recordDate" />
        <result column="Record_state" property="recordState" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        Record_id, User_id, AD_id, Record_date, Record_state
    </sql>

</mapper>
